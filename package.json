{
  "name": "@makerbi/vue3-colorpicker",
  "version": "2.4.2",
  "main": "dist/index.es.js",
  "types": "dist/types/index.d.ts",
  "type": "module",
  "author": "aesoper",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aesoper101/vue3-colorpicker/issues"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/aesoper101/vue3-colorpicker#readme",
  "scripts": {
    "dev": "vite",
    "build": "rimraf dist && vue-tsc --noEmit && vite build",
    "build:lib": "rimraf dist && vite build --config ./vite.config.lib.ts",
    "serve": "vite preview",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.cjs",
    "lint:pretty": "pretty-quick --staged",
    "prepare": "husky",
    "clean:dist": "rimraf dist",
    "pub": "cd dist && npm publish --registry=https://registry.npmjs.org --access public && cd ..",
    "unpub": "npm unpublish vue3-colorpicker --force --registry=https://registry.npmjs.org",
    "prelink": "cd dist && pnpm link && cd .. && pnpm link vue3-colorpicker",
    "deploy": "gh-pages -d storybook-static",
    "storybook": "storybook dev",
    "build-storybook": "rimraf storybook-static && storybook build",
    "reinstall": "rimraf node_modules && pnpm install"
  },
  "dependencies": {
    "@aesoper/normal-utils": "^0.1.5",
    "@popperjs/core": "^2.11.8",
    "@vueuse/core": "^11.2.0",
    "gradient-parser": "^1.0.2",
    "lodash-es": "^4.17.21",
    "tinycolor2": "^1.6.0",
    "vue": "^3.5.13",
    "vue-types": "^5.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/js": "^9.15.0",
    "@storybook/addon-actions": "^8.4.5",
    "@storybook/addon-docs": "^8.4.5",
    "@storybook/addon-essentials": "^8.4.5",
    "@storybook/addon-links": "^8.4.5",
    "@storybook/addon-storysource": "^8.4.5",
    "@storybook/cli": "^8.4.5",
    "@storybook/vue3": "^8.4.5",
    "@storybook/vue3-vite": "^8.4.5",
    "@types/gradient-parser": "^0.1.5",
    "@types/jest": "^29.5.14",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.9.1",
    "@types/tinycolor2": "^1.4.6",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitejs/plugin-vue": "^5.2.0",
    "@vitejs/plugin-vue-jsx": "^4.1.0",
    "@vue/compiler-sfc": "^3.5.13",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "babel-loader": "^9.2.1",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.15.0",
    "eslint-plugin-jest": "^28.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-storybook": "^0.11.1",
    "eslint-plugin-vue": "^9.31.0",
    "gh-pages": "^6.2.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.49",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-html": "^1.7.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.3.3",
    "pretty-quick": "^4.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^6.0.1",
    "sass": "^1.81.0",
    "sass-loader": "^16.0.3",
    "storybook": "^8.4.5",
    "stylelint": "^16.10.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-no-unsupported-browser-features": "^8.0.2",
    "stylelint-order": "^6.0.4",
    "typescript": "^5.6.3",
    "vite": "^5.4.11",
    "vite-plugin-dts": "^4.3.0",
    "vue-eslint-parser": "^9.4.3",
    "vue-loader": "^17.4.2",
    "vue-tsc": "^2.1.10"
  }
}
